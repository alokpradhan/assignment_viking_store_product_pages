<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<!-- END BOOTSTRAP -->


</head>
<body>
<div class="container">
  <div class="row">

    <div class="col-md-2">
    <%= render partial: '/admin/shared/navbar' %>
    </div>
    <div class="col-md-9">
      <h1 class="text-center">Viking Admin Portal</h1>
    <%= yield %>
      
    </div>
  </div>
</div>


</body>
</html>
